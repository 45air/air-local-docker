{
  "name": "@45air/air-local-docker",
  "version": "1.1.0",
  "description": "Local docker development environment setup and provisioning CLI",
  "license": "MIT",
  "author": "Reilly <r.lowery@45press.com>",
  "bugs": "https://github.com/45air/air-local-docker/issues",
